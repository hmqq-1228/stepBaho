/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

.tabbar{
  width: 100%;
  font-size: 22rpx;
  padding: 15rpx 0;
  background-color: #fff;
  border-top: 1rpx solid #eee;
  position: fixed;
  bottom: 0;
  left: 0;
  display: flex;
  justify-content:space-around;
}
.tabIconPic{
  width: 45rpx;
  height: 45rpx;
}
.tabbarText{
  text-align: center;
}

.tabbar2{
  width: 120rpx;
  position: fixed;
  right: 0rpx;
  bottom: 50rpx;
}
.tabbarItem2{
  width: 100rpx;
  height: 100rpx;
  border-radius: 50rpx;
  overflow: hidden;
  background-color: #fff;
  border: 1rpx solid #eee;
  margin-bottom: 20rpx;
}
.tabbarIcon2{
  width: 50rpx;
  padding-top: 8rpx;
  height: 50rpx;
  margin: 0 auto;
}
.tabIconPic2{
  width: 50rpx;
  height: 50rpx;
}
.tabbarText2{
  font-size: 24rpx;
  text-align: center;
}

.logoBox{
  margin-top: 100rpx;
}

/* 进度条 */
.progressBox{
  width: 100%;
  box-sizing: border-box;
  padding: 10rpx 20rpx;
  background-color: #fff;
  position: fixed;
  left: 0;
  top: 0;
}
/* 进度条修改备份 */
.newProgress{
  width: 100%;
  box-sizing: border-box;
  padding: 15rpx 0;
  background-color: #fff;
  position: fixed;
  left: 0;
  top: 0;
}
.myswiper{
  height: 75rpx
}
.bg_ball_Box{
  width: 100%;
  position: relative;
}
.bgLineBox{
  height: 40rpx;
  display: flex;
  justify-content: space-between;
}
/* 进度条修改备份 */
.bgLineBox2{
  height: 40rpx;
  display: flex;
  justify-content: space-between;
  _position: absolute;
  width: 100%;
}
.bgLineItem{
  width: 20%;
  height: 6rpx;
  background-color: #aaa;
  margin-top: 17rpx;
}
/* 进度条修改备份 */
.bgLineItem2{
  width: 100%;
  height: 6rpx;
  background-color: #aaa;
  margin-top: 17rpx;
}
.bgLineItem.active{
  background-color: #03cf3d;
}
/* 进度条修改备份 */
.bgLineItem2.active{
  background-color: #03cf3d;
}
.ballBox{
  width: 100%;
  position: absolute;
  left: 0;
  top: 0;
  display: flex;
  justify-content: space-between;
}
/* 进度条修改备份 */
.ballBox2{
  width: 100%;
  display: flex;
  justify-content: space-between;
}
/* 进度条修改备份 */
.mytest{
  _width: 120rpx;
  flex-grow: 1;
  position: relative;
}
.ballItem{
  width: 35rpx;
  height: 35rpx;
  border-radius: 20rpx;
  overflow: hidden;
  background-color: #aaa;
}
/* 进度条修改备份 */
.ballItem2{
  width: 35rpx;
  height: 35rpx;
  border-radius: 20rpx;
  overflow: hidden;
  background-color: #aaa;
  margin: 0 auto;
  position: absolute;
  top: 0;
  left: 50%;
  margin-left: -17rpx;
}
.ballItem.active{
  background-color: #03cf3d;
}
/* 进度条修改备份 */
.ballItem2.active{
  background-color: #03cf3d;
}
.ballDot{
  width: 7rpx;
  height: 7rpx;
  margin: 0 auto;
  margin-top: 14rpx;
  border-radius: 4rpx;
  overflow: hidden;
}
.ballDot.active{
  background-color: #fff;
}
.ballItemTextBox{
  font-size: 26rpx;
  display: flex;
  justify-content: space-between;
}
.ballText{
  font-size: 26rpx;
}
/* 进度条修改备份 */
.ballText2{
  font-size: 26rpx;
  text-align: center;
}

